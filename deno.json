{
  "$schema": "https://deno.land/x/deno@v2.0.0/deno_schema.json",
  "name": "plankapi",
  "version": "0.0.0",
  "description": "Planka API service",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "tasks": {
    "exec": "deno run --allow-all ./src/index.ts",
    "exec:env": "deno run --allow-all --env-file ./src/index.ts",
    "test": "deno run --allow-net --allow-read --allow-sys --allow-env --env-file test.ts"
  },
  "imports": {
    "planka-client": "https://raw.githubusercontent.com/GEWIS/planka-client/main/dist/planka-client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GEWIS/plankapi.git"
  },
  "author": "GEWIS GEFLITST",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GEWIS/plankapi/issues"
  },
  "homepage": "https://github.com/GEWIS/plankapi#readme"
}
